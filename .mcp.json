{
  "mcpServers": {
    "MCP_DOCKER": {
      "command": "docker",
      "args": ["mcp", "gateway", "run"]
    },
    "AWS_Documentation": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-aws-documentation"],
      "env": {}
    },
    "nuxt-ui": {
      "command": "npx",
      "args": ["-y", "mcp-remote", "https://ui.nuxt.com/mcp"]
    },
    "nx-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": ["nx", "mcp"]
    }
  }
}
