project_name = "riddle-rush-pwa"
aws_region   = "eu-central-1"

# Leave empty for auto-generated name (will be: riddle-rush-pwa-dev-ACCOUNT_ID)
bucket_name = ""

# Development typically doesn't use custom domain
# domain_name = ""
# certificate_arn = ""

cloudfront_price_class = "PriceClass_100"

