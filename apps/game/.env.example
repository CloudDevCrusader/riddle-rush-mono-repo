# Base URL for assets
BASE_URL=/

# Environment
NODE_ENV=development

# CloudWatch (optional)
CLOUDWATCH_ENDPOINT=
CLOUDWATCH_API_KEY=
DEBUG_ERROR_SYNC=false

# GitLab Feature Flags (optional)
# If not provided, falls back to local settings
# Format: https://gitlab.com/api/v4/feature_flags/unleash/:project_id
GITLAB_FEATURE_FLAGS_URL=https://gitlab.com/api/v4/feature_flags/unleash/77201558
GITLAB_FEATURE_FLAGS_TOKEN=your-instance-id-here
