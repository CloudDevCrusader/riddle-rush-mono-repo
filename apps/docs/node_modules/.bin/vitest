#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/cloudcrusader/projects/riddle-rush-nuxt-pwa/node_modules/.pnpm/vitest@3.2.4_@types+debug@4.1.12_@types+node@22.19.3_happy-dom@20.0.11_lightningcss@1.30.2_sass@1.97.1_terser@5.44.1/node_modules/vitest/node_modules:/home/cloudcrusader/projects/riddle-rush-nuxt-pwa/node_modules/.pnpm/vitest@3.2.4_@types+debug@4.1.12_@types+node@22.19.3_happy-dom@20.0.11_lightningcss@1.30.2_sass@1.97.1_terser@5.44.1/node_modules:/home/cloudcrusader/projects/riddle-rush-nuxt-pwa/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/cloudcrusader/projects/riddle-rush-nuxt-pwa/node_modules/.pnpm/vitest@3.2.4_@types+debug@4.1.12_@types+node@22.19.3_happy-dom@20.0.11_lightningcss@1.30.2_sass@1.97.1_terser@5.44.1/node_modules/vitest/node_modules:/home/cloudcrusader/projects/riddle-rush-nuxt-pwa/node_modules/.pnpm/vitest@3.2.4_@types+debug@4.1.12_@types+node@22.19.3_happy-dom@20.0.11_lightningcss@1.30.2_sass@1.97.1_terser@5.44.1/node_modules:/home/cloudcrusader/projects/riddle-rush-nuxt-pwa/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/vitest@3.2.4_@types+debug@4.1.12_@types+node@22.19.3_happy-dom@20.0.11_lightningcss@1.30.2_sass@1.97.1_terser@5.44.1/node_modules/vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/vitest@3.2.4_@types+debug@4.1.12_@types+node@22.19.3_happy-dom@20.0.11_lightningcss@1.30.2_sass@1.97.1_terser@5.44.1/node_modules/vitest/vitest.mjs" "$@"
fi
